{
  "name": "html-to-txt-bot",
  "description": "A Telegram bot that converts HTML files to TXT",
  "addons": [
    "heroku-postgresql"
  ],
  "buildpacks": [
    {
      "url": "heroku/python"
    }
  ],
  "env": {
    "TELEGRAM_API_ID": "<insert API ID here>",
    "TELEGRAM_API_HASH": "<insert API hash here>",
    "TELEGRAM_BOT_TOKEN": "<insert bot token here>"
  },
  "scripts": {
    "start": "python bot.py"
  }
}
